<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name ="BestBuy Test Automation Script" parallel="false">
<test name = "BestBuy Home">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="envName" value="browserstack"/>
    <parameter name="os" value="OS X"/>
    <parameter name="os_version" value="Sierra"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="80.0"/>
    <parameter name="url" value="https://www.bestbuy.com"/>

    <classes>
        <class name="BestBuyHome.TestBestBuyLandingPage"/>
        <class name="BestBuyHome.TestBestBuyDropDownMenu"/>
        <class name="BestBuyHome.TestBestBuyPageTitle"/>

    </classes>

</test>

</suite>